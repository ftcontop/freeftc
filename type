N:AddSlider("FOVSlider",{Title="FOV (degrees)",Description="Field of view for aimbot targeting",Default=60,Min=10,Max=180,Rounding=0,Callback=function(T)G.qbFOV=T end})
N:AddSlider("SmoothnessSlider",{Title="Aim Smoothness",Description="Lower = smoother aim",Default=1,Min=0.1,Max=1,Rounding=2,Callback=function(T)G.qbSmoothness=T end})

O:AddToggle("FlyToggle",{Title="Enable Fly",Description="Fly around the map (WASD + Space/Ctrl)",Default=false,Callback=function(T)G.flyEnabled=T if not T then local X=e.Character and e.Character:FindFirstChild("HumanoidRootPart")if X then X.Velocity=Vector3.zero end end end})
O:AddSlider("FlySpeedSlider",{Title="Fly Speed",Description="Adjust flying speed",Default=50,Min=10,Max=150,Rounding=0,Callback=function(T)G.flySpeed=T end})

Q:AddToggle("TackleReachToggle",{Title="Tackle Reach Enabled",Description="Extend your tackle range",Default=false,Callback=function(T)G.tackleReachEnabled=T end})
Q:AddSlider("TackleReachSlider",{Title="Tackle Reach Distance",Description="How far you can tackle from",Default=5,Min=1,Max=25,Rounding=1,Callback=function(T)G.tackleReachDistance=T end})
