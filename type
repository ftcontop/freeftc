pcall(function()C=loadstring(game:HttpGetAsync("https://github.com/ActualMasterOogway/Fluent-Renewed/releases/latest/download/Fluent.luau"))()D=loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/ActualMasterOogway/Fluent-Renewed/master/Addons/SaveManager.luau"))()E=loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/ActualMasterOogway/Fluent-Renewed/master/Addons/InterfaceManager.luau"))()end)

local G={pullEnabled=false,offsetDistance=3,pullSpeed=100,pullDistance=50,walkspeedMultiplier=1,walkspeedBase=16,qbAimbotEnabled=false,qbPredictionTime=0.15,qbFOV=60,qbAimKey=Enum.KeyCode.E,qbSmoothness=1,flyEnabled=false,flySpeed=50,tackleReachEnabled=false,tackleReachDistance=5,hitboxTackleReachEnabled=false,hitboxTackleReachDistance=5,jumpPowerEnabled=false,jumpPower=0,hitboxVisualizerEnabled=false,hitboxSize=1}

