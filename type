Q:AddSlider("HitboxSizeSlider",{Title="Hitbox Size",Description="Adjust your head size for hitting range",Default=1,Min=1,Max=3,Rounding=2,Callback=function(T)G.hitboxSize=T end})

D:SetLibrary(K)E:SetLibrary(K)D:IgnoreThemeSettings()D:SetIgnoreIndexes({})E:SetFolder("FTCHub")D:SetFolder("FTCHub/configs")E:BuildInterfaceSection(P)D:BuildConfigSection(P)K:SelectTab(1)
end

function V()if I then return end local Y=e:WaitForChild("PlayerGui")local Z=Instance.new("ScreenGui")Z.Name="FTCHub_MobileButton"Z.ResetOnSpawn=false Z.ZIndexBehavior=Enum.ZIndexBehavior.Sibling if J then Z.Size=UDim2.fromScale(1,1)end local a_=Instance.new("Frame")a_.Name="MainContainer"a_.Size=UDim2.new(0,120,0,140)a_.Position=UDim2.new(1,-140,0.5,-70)a_.BackgroundColor3=Color3.fromRGB(20,20,20)a_.BorderSizePixel=0 a_.Parent=Z local b_=Instance.new("UICorner")b_.CornerRadius=UDim.new(0,15)b_.Parent=a_ local c_=Instance.new("UIStroke")c_.Color=Color3.fromRGB(100,100,100)c_.Thickness=2 c_.Parent=a_ local d_=Instance.new("Frame")d_.Name="DragHeader"d_.Size=UDim2.new(1,0,0,30)d_.BackgroundColor3=Color3.fromRGB(30,30,30)d_.BorderSizePixel=0 d_.Parent=a_ local e_=Instance.new("UICorner")e_.CornerRadius=UDim.new(0,15)e_.Parent=d_ local f_=Instance.new("TextLabel")f_.Name="DragLabel"f_.Size=UDim2.new(1,-30,1,0)f_.BackgroundTransparency=1 f_.Text="FTCHub"f_.TextColor3=Color3.fromRGB(255,255,255)f_.TextSize=12 f_.Font=Enum.Font.GothamBold f_.Parent=d_ local g_=Instance.new("TextButton")g_.Name="CloseButton"g_.Size=UDim2.new(0,25,0,25)g_.Position=UDim2.new(1,-28,0,3)g_.BackgroundColor3=Color3.fromRGB(255,50,50)g_.BackgroundTransparency=0.1 g_.BorderSizePixel=0 g_.Text="X"g_.TextColor3=Color3.fromRGB(255,255,255)g_.TextSize=14 g_.Font=Enum.Font.GothamBold g_.Parent=d_ local h_=Instance.new("UICorner")h_.CornerRadius=UDim.new(0,8)h_.Parent=g_ local i_=Instance.new("TextButton")i_.Name="PullButton"i_.Size=UDim2.new(0.9,0,0,50)i_.Position=UDim2.new(0.05,0,0,40)i_.BackgroundColor3=Color3.fromRGB(0,100,255)i_.BackgroundTransparency=0.2 i_.BorderSizePixel=0 i_.Text="PULL"i_.TextColor3=Color3.fromRGB(255,255,255)i_.TextSize=18 i_.Font=Enum.Font.GothamBold i_.Parent=a_ local j_=Instance.new("UICorner")j_.CornerRadius=UDim.new(0,12)j_.Parent=i_ local k_=Instance.new("UIStroke")k_.Color=Color3.fromRGB(0,150,255)k_.Thickness=2 k_.Parent=i_ local l_,m_,n_,o_=false,nil,nil,nil d_.InputBegan:Connect(function(p_,q_)if q_ then return end if p_.UserInputType==Enum.UserInputType.Touch or p_.UserInputType==Enum.UserInputType.MouseButton1 then l_=true n_=p_.Position o_=a_.Position m_=p_.Changed:Connect(function()if l_ then local r_=p_.Position-n_ a_.Position=UDim2.new(o_.X.Scale,o_.X.Offset+r_.X,o_.Y.Scale,o_.Y.Offset+r_.Y)end end)end end)d_.InputEnded:Connect(function(p_,q_)if p_.UserInputType==Enum.UserInputType.Touch or p_.UserInputType==Enum.UserInputType.MouseButton1 then l_=false if m_ then m_:Disconnect()end end end)local s_=false i_.MouseButton1Click:Connect(function()s_=true end)i_.TouchTap:Connect(function()s_=true end)g_.MouseButton1Click:Connect(function()H=false W()U:SetValue(false)end)g_.TouchTap:Connect(function()H=false W()U:SetValue(false)end)I={gui=Z,button=i_,checkPull=function()if s_ then s_=false return true end return false end}Z.Parent=Y if C then C:Notify({Title="Mobile Button",Content="Pull button created! Drag to move, X to close.",Duration=3})end end

function W()if I then if I.gui then I.gui:Destroy()end I=nil if C then C:Notify({Title="Mobile Button",Content="Pull button removed.",Duration=2})end end end

local function t_()return e.Character end
local function u_()local v_=e.Character return v_ and v_:FindFirstChild("HumanoidRootPart")end
local function w_()local v_=e.Character return v_ and v_:FindFirstChildOfClass("Humanoid")end
local function x_(y_)if e.Team and y_.Team then return e.Team==y_.Team end return false end
local function z_()local u_=u_()if not u_ then return nil end local A_=nil local B_=nil local C_=u_.Position for _,D_ in ipairs(a:GetPlayers())do if D_~=e and D_.Character and not x_(D_)then local E_=D_.Character:FindFirstChild("HumanoidRootPart")local F_=D_.Character:FindFirstChildOfClass("Humanoid")if E_ and F_ and F_.Health>0 then local G_=(E_.Position-C_).Magnitude if G_<=100 then if not B_ or G_<B_ then A_=D_ B_=G_ end end end end end return A_ end

local H_,I_,J_=nil,0,0.5
local function K_()local L_=tick()if H_ and(L_-I_)<J_ then if H_.Parent then return H_ else H_=nil end end local M_={}for _,N_ in ipairs(e:GetService("Workspace"):GetDescendants())do if N_:IsA("BasePart")then local O_=N_.Name:lower()if O_:find("ball")or O_:find("football")then table.insert(M_,N_)end elseif N_:IsA("Model")then local O_=N_.Name:lower()if O_:find("ball")or O_:find("football")then local P_=N_:FindFirstChildWhichIsA("BasePart")if P_ then table.insert(M_,P_)end end end end local Q_,R_=nil,nil local u_=u_()if not u_ then return nil end local C_=u_.Position for _,S_ in ipairs(M_)do local T_=(S_.Position-C_).Magnitude if not R_ or T_<R_ then Q_=S_ R_=T_ end end if Q_ then H_=Q_ I_=L_ end return Q_ end

b.InputBegan:Connect(function(U_,V_)if V_ then return end if U_.UserInputType==Enum.UserInputType.MouseButton1 then l=true end if U_.KeyCode==G.qbAimKey then i=true end if J and I and I.checkPull()then l=true end end)
b.InputEnded:Connect(function(U_,V_)if U_.UserInputType==Enum.UserInputType.MouseButton1 then l=false end if U_.KeyCode==G.qbAimKey then i=false end end)

d.Heartbeat:Connect(function(W_)local v_=t_()local F_=w_()if F_ then if R then F_.WalkSpeed=16*G.walkspeedMultiplier else F_.WalkSpeed=16 end end if G.pullEnabled and l then local u_=u_()if u_ then local X_=K_()if X_ and X_.Parent then local Y_=X_.Position local Z_=u_.Position local a0=(Y_-Z_).Magnitude if a0<=G.pullDistance and a0>G.offsetDistance then local b0=(Y_-Z_).Unit local c0=G.pullSpeed*W_ local d0=math.min(c0,a0-G.offsetDistance)u_.CFrame=u_.CFrame+(b0*d0)end end end end if G.hitboxVisualizerEnabled then if v_ then local e0=v_:FindFirstChild("Head")if e0 then e0.Size=Vector3.new(2*G.hitboxSize,2*G.hitboxSize,2*G.hitboxSize)e0.Transparency=0.5 end end for _,f0 in pairs(a:GetPlayers())do if f0~=e and f0.Character then local g0=f0.Character:FindFirstChild("Head")if g0 then g0.Size=Vector3.new(2*G.hitboxSize,2*G.hitboxSize,2*G.hitboxSize)g0.Transparency=0.5 end end end else if v_ then local e0=v_:FindFirstChild("Head")if e0 then e0.Size=Vector3.new(2,2,2)e0.Transparency=0 end end for _,f0 in pairs(a:GetPlayers())do if f0~=e and f0.Character then local g0=f0.Character:FindFirstChild("Head")if g0 then g0.Size=Vector3.new(2,2,2)g0.Transparency=0 end end end end local u_=t_()and t_():FindFirstChild("HumanoidRootPart")if u_ and m then if not S then S=Instance.new("BodyForce",u_)S.Name="JumpBoost"end local h0=G.jumpPower/100*80 S.Force=Vector3.new(0,h0*100,0)else if S then S:Destroy()S=nil end end if G.qbAimbotEnabled and i then local i0=z_()if i0 and i0.Character then local E_=i0.Character:FindFirstChild("HumanoidRootPart")if E_ then local u_=u_()if u_ then local j0=E_.Position local k0=E_.AssemblyLinearVelocity or E_.Velocity local l0=j0+k0*G.qbPredictionTime local m0,n0=d.Camera:WorldToViewportPoint(l0)local o0=b:GetMouseLocation()local p0=m0.X-o0.X local q0=m0.Y-o0.Y local r0=math.sqrt(p0*p0+q0*q0)local s0=(G.qbFOV/180)*math.min(d.Camera.ViewportSize.X,d.Camera.ViewportSize.Y)/2 if n0 and r0<=s0 then local t0=(l0-d.Camera.CFrame.Position).Unit local u0=CFrame.new(d.Camera.CFrame.Position,d.Camera.CFrame.Position+t0)d.Camera.CFrame=d.Camera.CFrame:Lerp(u0,G.qbSmoothness)end end end end end if G.flyEnabled then local u_=u_()if u_ then local v0=Vector3.zero local w0=d.Camera local x0=w0.CFrame if b:IsKeyDown(Enum.KeyCode.W)then v0=v0+x0.LookVector end if b:IsKeyDown(Enum.KeyCode.S)then v0=v0-x0.LookVector end if b:IsKeyDown(Enum.KeyCode.A)then v0=v0-x0.RightVector end if b:IsKeyDown(Enum.KeyCode.D)then v0=v0+x0.RightVector end if b:IsKeyDown(Enum.KeyCode.Space)then v0=v0+Vector3.new(0,1,0)end if b:IsKeyDown(Enum.KeyCode.LeftControl)then v0=v0+Vector3.new(0,-1,0)end if v0.Magnitude>0 then v0=v0.Unit end u_.Velocity=v0*G.flySpeed end end end)
