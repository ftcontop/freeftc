if C then
local K=C:CreateWindow({Title="FTCHub",SubTitle="UF Script",TabWidth=100,Size=UDim2.fromOffset(500,380),Acrylic=false,Theme="Dark",MinimizeKey=Enum.KeyCode.RightShift})
C:Notify({Title="FTCHub",Content="UI successfully loaded.",Duration=4})

local L=K:AddTab({Title="Player",Icon="user"})
local M=K:AddTab({Title="Pull Vector",Icon="magnet"})
