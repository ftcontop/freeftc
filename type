local R,S=false,nil

L:AddToggle("WalkspeedToggle",{Title="Enable WalkSpeed",Description="Toggle walkspeed modification",Default=false,Callback=function(T)R=T end})
L:AddSlider("WalkspeedSlider",{Title="WalkSpeed Multiplier",Description="Adjust your character's walk speed",Default=1,Min=0.5,Max=5,Rounding=2,Callback=function(T)G.walkspeedMultiplier=T end})
L:AddToggle("JumpPowerToggle",{Title="Enable Jump Power",Description="Toggle jump power boost",Default=false,Callback=function(T)m=T end})
L:AddSlider("JumpPowerSlider",{Title="Jump Power",Description="Adjust jump height",Default=0,Min=0,Max=100,Rounding=1,Callback=function(T)G.jumpPower=T end})

M:AddToggle("PullToggle",{Title="Enable Pull",Description="Hold M1 to pull to ball",Default=false,Callback=function(T)G.pullEnabled=T end})
M:AddSlider("OffsetSlider",{Title="Offset Distance",Description="Distance to stop before ball",Default=3,Min=0,Max=10,Rounding=1,Callback=function(T)G.offsetDistance=T end})
M:AddSlider("PullSpeedSlider",{Title="Pull Speed",Description="How fast you move to the ball",Default=100,Min=20,Max=200,Rounding=5,Callback=function(T)G.pullSpeed=T end})
M:AddSlider("PullDistanceSlider",{Title="Pull Distance",Description="Maximum distance to pull from",Default=50,Min=10,Max=150,Rounding=5,Callback=function(T)G.pullDistance=T end})
local U=M:AddToggle("MobileButtonToggle",{Title="Mobile Pull Button",Description="Show on-screen pull button for mobile",Default=false,Callback=function(T)H=T if T then V()else W()end end})

N:AddToggle("AimbotToggle",{Title="Enable QB Aimbot",Description="Automatically aim at targets",Default=false,Callback=function(T)G.qbAimbotEnabled=T end})
